javascript:
(function(){
	var getReportAndGetFarmData = function() {
		var element = document.createElement('script');
		element.src = 'http://www.stormtide.net/ingress/scripts/getFarmData.js';
		element.async = true;
		var alreadyGotData = false;
		element.onreadystatechange = element.onload = function() {
			var state = s.readyState;
			if (!alreadyGotData && (!state || /loaded|complete/.test(state))) {
				alreadyGotData = true;
				getFarmData();
			}
		};
		element.type = 'text/javascript';
		document.getElementsByTagName('head')[0].appendChild(element);
	};
	getReportAndGetFarmData();
	setInterval(getReportAndGetFarmData, 60000);
	void(0);
})();
